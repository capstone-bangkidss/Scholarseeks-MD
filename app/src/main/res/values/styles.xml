<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="PoppinsSemiBold">
        <item name="android:fontFamily">@font/poppins_semibold</item>
    </style>

    <style name="text">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    
    <style name="text.journalTitle">
        <item name="android:fontFamily">@font/poppins_semibold</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textAlignment">textStart</item>
    </style>

    <style name="text.light10sp">
        <item name="android:fontFamily">@font/poppins_light</item>
        <item name="android:textSize">10sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textAlignment">textStart</item>
    </style>

    <style name="text.regular10sp">
        <item name="android:fontFamily">@font/poppins_regular</item>
        <item name="android:textSize">10sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textAlignment">textStart</item>
    </style>

    <style name="text.regular16sp">
        <item name="android:fontFamily">@font/poppins_regular</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textAlignment">textStart</item>
    </style>
</resources>