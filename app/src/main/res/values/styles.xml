<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="PoppinsSemiBold">
        <item name="android:fontFamily">@font/poppins_semibold</item>
    </style>

    <style name="text">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    
    <style name="text.journalTitle">
        <item name="android:fontFamily">@font/poppins_semibold</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textAlignment">textStart</item>
    </style>

    <style name="text.regular14sp">
        <item name="android:fontFamily">@font/poppins_light</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textAlignment">textStart</item>
    </style>

    <style name="text.Bold20sp">
        <item name="android:fontFamily">@font/poppins_bold</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textAlignment">textStart</item>
    </style>
</resources>